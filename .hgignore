syntax: glob

# ignore temporary and compiled files
\#*\#
*~
__pycache__/*
*.out
\.~lock*
.pytest_cache*

# ignore data output from learning, such as weight files
*.pt
*.pkl
*.h5

# ignore hydra-generated files
*.hydra*

# ignore datasets
datasets/*

# ignore experimental records and images
*.csv
*.png
*.db

# ignore html renderings of routes
*.html

# ignore commit message files
commit.txt

# ignore vscode's state
.vscode

# ignore tensorboard logs and loger logs
*tfevents*
*.log

# ignore temporary backup files
*.bak

# ignore temporary git repo
*.git

# ignore figure directory
figs
